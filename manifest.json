{
  "name": "Url Shortener",
  "version": "1.0",
  "description": "can short your URL use \"v.ht\" website by this extension | by Saif",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "activeTab",
    "bookmarks"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "icons": {
    "128": "logo.png"
  },
  "manifest_version": 2
}